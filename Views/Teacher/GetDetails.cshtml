@model IEnumerable< TermProject1._1.Models.Student>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section Menu
{
    <ul>
    <li><a href="/Home/Index">Home</a></li>
    <li><a href="/Home/Logout">Logout</a></li>
    <li><a href="/Home/AboutUs">About Us</a></li>
    <li><a href="/Home/ContactUs">Contact Us</a></li>
        </ul>
    
}

<!DOCTYPE html>

<html>
<head>
    <title>Student Details</title>
    <meta charset="utf-8">
    <meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />
    <meta http-equiv="imagetoolbar" content="no" />
    <link rel="stylesheet" href="~/Content/layout.css" type="text/css" />
    <link href="~/Content/featured_slide.css" rel="stylesheet" />
    <link href="~/Content/forms.css" rel="stylesheet" />
    <link href="~/Content/loginslider.css" rel="stylesheet" />
    <link href="~/Content/navi.css" rel="stylesheet" />
    <link href="~/Content/tables.css" rel="stylesheet" />
    <script type="text/javascript" src="~/Scripts/jquery-1.4.1.min.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery.slidepanel.setup.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery.cycle.min.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery.cycle.setup.js"></script>
    <link rel="stylesheet" href="../Content/reset.css" type="text/css" media="screen">
    <link rel="stylesheet" href="../Content/style.css" type="text/css" media="screen">
    <link rel="stylesheet" href="../Content/grid.css" type="text/css" media="screen">   
    <script src="../Scripts/cufon-yui.js" type="text/javascript"></script>
    <script src="../Scripts/cufon-replace.js" type="text/javascript"></script>
    <script src="../Scripts/NewsGoth_400.font.js" type="text/javascript"></script>
	<script src="../Scripts/NewsGoth_700.font.js" type="text/javascript"></script>
    <script src="../Scripts/NewsGoth_Lt_BT_italic_400.font.js" type="text/javascript"></script>
    <script src="../Scripts/Vegur_400.font.js" type="text/javascript"></script> 
    <script src="../Scripts/FF-cash.js" type="text/javascript"></script>     
    <script type="text/javascript">
    </script>
</head>
<body>
<div class="wrapper col3">
  <div id="homecontent">
    <div class="fl_left">
      <div class="column2">
   @foreach (var x in Model)                           
    {
    <table style="margin-right:400px" border="1">

            
                
         <tr><td><p>Student ID: </p></td><td>@x.Id</td></tr>
          
          <tr><td><p >Student Name: </p></td><td> @x.name</td></tr>
        <tr><td><p>Files Uploaded</p></td><td>
        @foreach (var y in x.FilesUploadeds)
        { 
                    @y.filepath<p>:</p>
        }
         </td>  
        </tr>
          <tr><td><p >Student Username :</p></td><td>@x.username</td></tr>
		  <tr><td><p>Student's Email :</p> </td><td>@x.email</td></tr>
          <tr><td><p>Students Age : </p> </td><td>@x.age</td></tr>
        <tr><td><p>Students DOB : </p> </td><td>@x.dob</td></tr>
        <tr><td><p>Students Gender : </p> </td><td>@x.gender</td></tr>
        <tr><td><p>Students's Teacher ID : </p> </td><td>@x.TId</td></tr>
      </table>
                                }
                                    
                                    </div>
                                    </div>
                                    </div>
                                    </div>
</body>
</html>
